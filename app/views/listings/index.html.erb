<div class="container">

<h1>Listing Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Processing time</th>
      <th>View count</th>
      <th>Status</th>
      <th>Slug</th>
      <th>User</th>
      <th>Subcategory</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.name %></td>
        <td><%= listing.description %></td>
        <td><%= listing.price %></td>
        <td><%= listing.processing_time %></td>
        <td><%= listing.view_count %></td>
        <td><%= listing.status %></td>
        <td><%= listing.slug %></td>
        <td><%= listing.user %></td>
        <td><%= listing.subcategory %></td>
        <td><%= listing.location %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>

</div>